{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "linterOptions": {
    "exclude": [
      "tests"
    ]
  },
  "jsRules": {},
  "rules": {
    "interface-name": [
      true,
      "never-prefix"
    ],
    "object-literal-sort-keys": false,
    "forin": false,
    "indent": [
      true,
      "spaces",
      2
    ],
    "ordered-imports": false,
    "no-console": false,
    "max-line-length": [
      true,
      200
    ]
  },
  "rulesDirectory": []
}